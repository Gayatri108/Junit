package com.java.app;

import java.util.Arrays;

public class Course {
	public String[] displayCourse()
	{
		String[] courseName={"c","c++","java","HTML"};
		System.out.println("Course Name are:"+Arrays.toString(courseName));
		return courseName;
	}
	public String[] addCourse()
	{
		String[] courseName={"c","c++","java","HTML","sql"};
		System.out.println("Course Name are:"+Arrays.toString(courseName));
		return courseName;
	}

}
